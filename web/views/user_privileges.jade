extends layout

block content
  h1= title
  p Welcome to #{title}

  h2 Users Listing
  table
    thead
      tr
        th First Name
        th Last Name
        th Level
        th
    tbody
      each row in users
        tr
          each item in row
            td(id="class-" + item) #{item}  
          td 
            button(type='submit', onclick='changePrivilege("#{row}")') Change Privilege