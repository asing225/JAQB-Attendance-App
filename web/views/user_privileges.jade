extends layout

block content

  div(class = "w3-row-padding")
    div(class = "w3-col w3-white w3-padding")
      h2 Users Listing
      table(id="mytab", class="table table-striped table-bordered")
        thead(align="left")
          tr
            th First Name
            th Last Name
            th Level
            th
        tbody
          each row in users
            tr
              each item in row
                td(id="class-" + item) #{item}  
              td 
                button(type='submit', onclick='changePrivilege("#{row}")') Change Privilege